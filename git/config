[alias]
	a    = add
	br   = branch
	c    = commit
	ch   = checkout
	ci   = commit
	co   = checkout
	clone= clone --depth=1
	d    = diff
	dc   = diff --cached
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	ol   = log --oneline
        lg   = "log --format='%C(auto) %aN %h %d %s'"
	ph   = push
	pl   = pull
	ps   = push
	s    = status --ignored
	st   = status --ignored
	sw   = switch
	last = for-each-ref 'refs/heads' 'refs/remotes/*/[^H][^E][^A][^D]*' --sort=-committerdate --format='%(HEAD) %(refname:short) - (%(color:green)%(committerdate:relative)%(color:reset)) - %(color:yellow)%(authorname)%(color:reset) - %(contents:subject)'
	del-remote-br = !sh -c 'git push origin :$1' -
[color]
	ui = auto
[core]
	autocrlf = input
[diff "odf"]
        binary = true
        textconv = odt2txt
[fetch]
	prune = true
[help]
	autocorrect = 25
[init]
	defaultBranch = master
[log]
	mailmap = true
[mailmap]
	file = ~/.config/git/mailmap
[pager]
	branch = false
[push]
	default = simple
[user]
	name = Collin Richards
	email = richardscollin@gatech.edu
